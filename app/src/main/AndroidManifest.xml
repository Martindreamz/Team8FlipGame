<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="iss.workshop.team8flipgame">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.VideoOnStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CreditsActivity" />

        <service android:name=".service.BGMusicService" />

        <activity android:name=".activity.GameActivity" />
        <activity android:name=".activity.ImagePickingActivity" />
        <activity android:name=".activity.LeaderBoardActivity" />
        <activity android:name=".activity.HomeActivity">
        </activity>
    </application>

</manifest>